	<!--Drying racks-->
	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>DankPyon_DryingRack1x2c</defName>
		<label>drying rack(1x2)</label>
		<description>A wooden rack to dry hay under the sun for preservation and long time use.</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Deco/DryingRack1x2</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<altitudeLayer>Building</altitudeLayer>
		<canBeUsedUnderRoof>false</canBeUsedUnderRoof>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.5</fillPercent>
		<pathCost>70</pathCost>
		<statBases>
			<WorkToBuild>500</WorkToBuild>
			<Mass>5</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,1)</size>
		<costList>
			<WoodLog>50</WoodLog>
		</costList>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_NotUnderRoof</li>
		</placeWorkers>
		<designationCategory>MV_Deco</designationCategory>
	</ThingDef>
