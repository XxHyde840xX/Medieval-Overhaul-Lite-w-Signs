<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--Deco Misc-->
	<ThingDef ParentName="BuildingBase">
		<defName>DankPyon_BarrelWater</defName>
		<label>water filled barrel</label>
		<description>A barrel filled with water to clean oneself or the surroundings</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Deco/BarrelWater</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<drawRotated>false</drawRotated>
		</graphicData>
		<size>(1,1)</size>
		<uiIconScale>0.75</uiIconScale>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.3</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<pathCost>30</pathCost>
		<costList>
			<Steel>5</Steel>
		</costList>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<Flammability>1</Flammability>
			<Beauty>0.5</Beauty>
			<Cleanliness>5</Cleanliness>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">3</StyleDominance>
		</statBases>
		<designationCategory>MV_Deco</designationCategory>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<comps>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<Comfort>0.05</Comfort>
				</statOffsets>
				<maxSimultaneous>1</maxSimultaneous>
				<maxDistance>6</maxDistance>
			</li>
			<li Class="CompProperties_Styleable"/>
		</comps>
	</ThingDef>
